{
  "expo": {
    "name": "Aeris",
    "slug": "Aeris",
    "version": "1.0.0",
    "scheme": "aeris",
    "orientation": "portrait",
    "icon": "./assets/weather.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/weather.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "Y"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/weather.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.san_thosh2.Aeris"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "permissions": [
      "ACCESS_FINE_LOCATION"
    ],
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3eb779be-6a1c-4464-bbd3-46b9911a884e"
      }
    }
  }
}
